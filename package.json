{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "dependencies": {
    "ajv": "6.12.6",
    "express": "4.18.1",
    "handlebars": "4.7.7",
    "less": "4.1.3"
  },
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/handlebars": "4.1.0",
    "@types/jsdom": "20.0.0",
    "@types/mocha": "9.1.1",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "@webpack-cli/generators": "2.5.0",
    "chai": "4.3.6",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "eslint": "8.21.0",
    "eslint-config-airbnb": "19.0.4",
    "handlebars-loader": "^1.7.2",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "less-loader": "11.0.0",
    "mini-css-extract-plugin": "2.6.1",
    "mocha": "10.0.0",
    "postcss-less": "6.0.0",
    "sinon": "14.0.0",
    "style-loader": "3.3.1",
    "stylelint": "14.9.1",
    "stylelint-config-standard": "26.0.0",
    "stylelint-less": "1.0.6",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "scripts": {
    "dev": "parcel watch --no-cache --dist-dir build src/index.html",
    "build": "webpack --mode=production --node-env=production",
    "start": "npm run build && node server-src/app.js",
    "start:server": "node server-src/app.js",
    "lint": "eslint ./src/**/*.ts && stylelint ./src/**/*.less",
    "lint:fix": "eslint --fix ./src/**/*.ts && stylelint --fix ./src/**/*.less",
    "test": "mocha",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all test lint",
      "pre-push": "npm run test lint"
    }
  },
  "staticFiles": {
    "staticPath": "static",
    "staticOutPath": "static",
    "watcherGlob": "**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexandrGrents/middle.messenger.praktikum.yandex.git"
  },
